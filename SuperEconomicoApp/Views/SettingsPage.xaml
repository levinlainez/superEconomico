<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SuperEconomicoApp.Views.SettingsPage">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Administrar Categorias" x:Name="ButtonCategories"
                    Clicked="ButtonCategories_Clicked"/>
            <Button Text="Administrar Producto" x:Name="ButtonProducts"
                    Clicked="ButtonProducts_Clicked"/>
            <Button Text="Administrar Cart" x:Name="ButtonCart"
                    Clicked="ButtonCart_Clicked"/>


        </StackLayout>
    </ContentPage.Content>
</ContentPage>